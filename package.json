{
  "name": "sass-responsive-util",
  "version": "1.0.6",
  "description": "A modern Sass utility library for DPI-aware responsive design. Easily convert between px, pt, rem, vw, and clamp() with scalable typography support.",
  "main": "_index.scss",
  "sass": "_index.scss",
  "exports": {
    ".": "./_index.scss",
    "./functions": "./functions/_index.scss",
    "./setting": "./setting/_index.scss"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minori-003/sass-responsive-util.git"
  },
  "bugs": {
    "url": "https://github.com/minori-003/sass-responsive-util/issues"
  },
  "homepage": "https://github.com/minori-003/sass-responsive-util#readme",
  "keywords": [
    "sass",
    "scss",
    "responsive-design",
    "clamp",
    "dpi",
    "pt",
    "rem",
    "vw",
    "em",
    "percent",
    "fluid-typography",
    "frontend",
    "sass-library",
    "design-system",
    "utility"
  ],
  "author": {
    "name": "Minori Koishi",
    "email": "",
    "url": "https://github.com/minori-003"
  },
  "license": "MIT",
  "files": [
  "_index.scss",
  "functions/**/*.scss",
  "setting/**/*.scss",
  "README.md",
  "LICENSE"
  ],
  "engines": {
    "sass": ">=1.56.0"
  },
  "scripts": {
  "build:sass": "sass test.scss test.css --load-path=node_modules",
  "watch:sass": "sass --watch test.scss test.css --load-path=node_modules"
  },
  "publishConfig": {
    "access": "public"
  }
}
